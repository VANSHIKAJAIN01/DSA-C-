#include <iostream>

using namespace std;

bool findkey(int arr[],int s,int k){
    //linear search
    for(int i=0;i<s;i++){
        if(arr[i]==k)
        return true;
    }
    return false;
}
int main()
{
    //array declare
    int arr[20]={1,2,3,4,5};
    int size =5;
    cout<<"ENETER THE KEY TO FIND : ";
    int key;
    cin>>key;
    if(findkey(arr,size,key)){
        cout<<" FOUND ";
    }
    else
    {
        cout<<" NOT FOUND ";
    }

    return 0;
}
